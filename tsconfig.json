{
  "exclude": [
    "node_modules",
    "dist",
    "tests",
    "jest.config.ts"
  ],
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "src",
    "baseUrl": "./",
    "paths": {},
    "resolveJsonModule": true,
    "outDir": "dist/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}