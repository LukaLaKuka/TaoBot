// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Guild {
  id   Int @id @default(autoincrement())
  guild_id String @unique
  name String
}

model Rhytm {
  id Int @id @default(autoincrement())

}

model BelovedUser {
  id   String @id @unique
}

